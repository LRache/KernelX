{
    "version": "0.2.0",
    "configurations": [
    {
        "name": "KernelX",
        "type": "cppdbg",
        "request": "launch",
        "cwd": "${workspaceFolder}",
        "program": "${workspaceFolder}/build/qemu-virt-riscv64/vmkernelx",
        "MIMode": "gdb",
        "miDebuggerPath": "gdb-multiarch",
        "miDebuggerServerAddress": "127.0.0.1:1234",
        "miDebuggerArgs": "-ex 'break *0x80200000'",
        "externalConsole": true,
        "stopAtEntry": true
    }
    ]
}